{
  "suggest.noselect": true,
  "suggest.enablePreselect": false,
  "suggest.completionItemKindLabels": {
    "text": "t",
    "method": "m",
    "function": "f",
    "constructor": "c",
    "field": "F",
    "variable": "v",
    "class": "C",
    "interface": "I",
    "module": "M",
    "property": "p",
    "unit": "u",
    "value": "V",
    "enum": "E",
    "keyword": "k",
    "snippet": "s",
    "color": "c",
    "file": "f",
    "reference": "r",
    "folder": "d",
    "enumMember": "m",
    "constant": "c",
    "struct": "S",
    "event": "e",
    "operator": "o",
    "typeParameter": "T"
  },
  "diagnostic.errorSign": "✘",
  "diagnostic.warningSign": "▲",
  "diagnostic.infoSign": "ℹ",
  "diagnostic.hintSign": "➤",
  "diagnostic.virtualText": false,
  "diagnostic.virtualTextCurrentLineOnly": true,
  "diagnostic.checkCurrentLine": false,
  "diagnostic.messageTarget": "echo",
  "diagnostic.enableMessage": "never",
  "diagnostic.autoRefresh": true,
  "diagnostic.maxWindowHeight": 20,
  "diagnostic.maxWindowWidth": 80,
  "diagnostic.floatConfig": {
    "border": true,
    "rounded": true,
    "focusable": true,
    "shadow": true,
    "title": " Diagnostics ",
    "close": true,
    "maxHeight": 20,
    "maxWidth": 80
  },
  "hover.floatConfig": {
    "border": true,
    "rounded": true,
    "focusable": true,
    "shadow": true,
    "title": " Documentation ",
    "close": false,
    "maxHeight": 30,
    "maxWidth": 100,
    "highlight": "CocFloating",
    "borderhighlight": "CocFloatingBorder",
    "autoHide": false
  },
  "hover.target": "float",
  "signature.floatConfig": {
    "border": true,
    "rounded": true,
    "shadow": true,
    "title": " Signature "
  },
  "suggest.floatConfig": {
    "border": true,
    "rounded": true,
    "winblend": 0,
    "shadow": true,
    "borderhighlight": "CocFloatingBorder"
  },
  "suggest.autoTrigger": "always",
  "suggest.timeout": 5000,
  "suggest.minTriggerInputLength": 1,
  "suggest.acceptSuggestionOnCommitCharacter": true,
  "python.linting.enabled": true,
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": false,
  "python.linting.ruffEnabled": true,
  "python.formatting.provider": "ruff",
  "python.analysis.typeCheckingMode": "basic",
  "python.analysis.autoImportCompletions": true,
  "python.analysis.autoSearchPaths": true,
  "python.analysis.diagnosticMode": "openFilesOnly",
  "python.analysis.useLibraryCodeForTypes": true,
  "pyright.inlayHints.variableTypes": false,
  "pyright.inlayHints.functionReturnTypes": false,
  "pyright.inlayHints.parameterTypes": false,
  "coc.preferences.formatOnSave": false,
  "coc.preferences.formatOnType": false,
  "coc.preferences.jumpCommand": "edit",
  "coc.preferences.messageLevel": "error",
  "coc.preferences.extensionUpdateCheck": "weekly",
  "coc.preferences.bracketEnterImprove": true,
  "coc.preferences.useQuickfixForLocations": false,
  "coc.preferences.snippetStatusText": "Ⓢ ",
  "coc.preferences.floatActions": true,
  "coc.preferences.currentFunctionSymbolAutoUpdate": false,
  "coc.preferences.colorSupport": true,
  "workspace.ignoredFolders": [
    "$HOME",
    "$HOME/.cargo/**",
    "$HOME/.rustup/**",
    "/usr/local/**"
  ],
  "list.insertMappings": {
    "<C-c>": "do:exit"
  },
  "list.normalMappings": {
    "<C-c>": "do:exit"
  },
  "snippets.ultisnips.pythonPrompt": false,
  "pairs.enableCharacters": ["(", "[", "{", "'", "\"", "`"],
  "pairs.disableLanguages": ["markdown"],
  "highlight.colorNames": true,
  "highlight.document.enable": true,
  "highlight.colors.enable": true,
  "semanticTokens.enable": true
}
